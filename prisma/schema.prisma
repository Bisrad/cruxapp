generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider     = "mysql"
  url          = env("DATABASE_URL")
  relationMode = "prisma"
}

model Profile {
  id     Int     @id @default(autoincrement())
  bio    String?
  userId Int     @unique

  @@index([userId])
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}

model Area {
  id          Int     @id
  name        String? @db.VarChar(255)
  location    String? @db.VarChar(255)
  description String? @db.Text
}

model Route {
  id         Int     @id
  wall_id    Int?
  name       String? @db.VarChar(255)
  difficulty String? @db.VarChar(5)
}

model Wall {
  id      Int     @id
  area_id Int?
  name    String? @db.VarChar(255)
}

model vt_HOLD_1930360de2fc517da98465f78bef370b_20230705044736 {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime
  title     String   @db.VarChar(255)
  content   String?
  published Boolean  @default(false)
  authorId  Int

  @@index([authorId], map: "Post_authorId_idx")
  @@map("_vt_HOLD_1930360de2fc517da98465f78bef370b_20230705044736")
}

model vt_HOLD_ccb647ff268b5e99a59af283a2c926d8_20230705044735 {
  id          Int     @id @default(autoincrement())
  name        String  @db.VarChar(255)
  image_url   String? @db.VarChar(255)
  category_id Int?

  @@index([category_id], map: "category_id_idx")
  @@map("_vt_HOLD_ccb647ff268b5e99a59af283a2c926d8_20230705044735")
}

model vt_HOLD_1b2b55f24b3d5b659613d17b2b0563eb_20230705061010 {
  id   Int    @id @default(autoincrement())
  name String @db.VarChar(255)

  @@map("_vt_HOLD_1b2b55f24b3d5b659613d17b2b0563eb_20230705061010")
}
